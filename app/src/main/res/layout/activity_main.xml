<android.support.v4.widget.DrawerLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/drawer"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fitsSystemWindows="true"
    tools:context=".MainActivity">

    <RelativeLayout
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical"
        >



        <FrameLayout
            android:id="@+id/frame"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <FrameLayout
                android:id="@+id/frame2"
                android:layout_width="match_parent"
                android:layout_height="match_parent">



                <FrameLayout
                    android:id="@+id/frame3"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"/>

                <include
                    android:id="@+id/toolbar"
                    layout="@layout/tool_bar"
                    />

                <com.github.clans.fab.FloatingActionMenu
                    android:id="@+id/fabrating"
                    app:menu_icon="@drawable/star"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_alignParentRight="false"
                    android:layout_marginTop="50dp"
                    android:layout_marginLeft="0dp"
                    android:layout_gravity="right"
                    android:paddingRight="10dp"
                    android:paddingTop="10dp"
                    android:paddingBottom="10dp"
                    android:paddingLeft="10dp"
                    android:elevation="9dp"
                    app:menu_buttonSpacing="7dp"
                    app:menu_labels_ellipsize="end"
                    app:menu_labels_singleLine="true"
                    app:menu_fab_size="mini"
                    app:menu_openDirection="down"
                    app:menu_colorNormal="@color/ColorPrimary3"
                    app:menu_colorPressed="@color/ColorPrimary3"
                    app:menu_showShadow="false">

                    <com.github.clans.fab.FloatingActionButton
                        android:id="@+id/hilowrating"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/chevron_right"
                        app:fab_size="mini"
                        app:fab_colorNormal="@color/ColorAccent"
                        app:fab_colorPressed="@color/ColorAccentDark" />
                    <!--app:fab_label="High/Low"-->

                    <com.github.clans.fab.FloatingActionButton
                        android:id="@+id/lowhirating"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/chevron_left"
                        app:fab_size="mini"
                        app:fab_colorNormal="@color/ColorAccent"
                        app:fab_colorPressed="@color/ColorAccentDark"/>
                    <!--app:fab_label="Low/High"-->

                    <com.github.clans.fab.FloatingActionButton
                        android:id="@+id/rangerating"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/code_tags"
                        app:fab_size="mini"
                        app:fab_colorNormal="@color/ColorAccent"
                        app:fab_colorPressed="@color/ColorAccentDark"/>
                    <!--app:fab_label="Range"-->

                </com.github.clans.fab.FloatingActionMenu>
                <com.github.clans.fab.FloatingActionMenu
                    android:id="@+id/fabcontent"
                    app:menu_icon="@drawable/water"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_alignParentRight="false"
                    android:layout_marginTop="50dp"
                    android:layout_marginLeft="0dp"
                    android:paddingRight="10dp"
                    android:paddingTop="10dp"
                    android:paddingBottom="10dp"
                    android:paddingLeft="10dp"
                    android:elevation="9dp"
                    app:menu_buttonSpacing="7dp"
                    app:menu_labels_ellipsize="end"
                    app:menu_labels_singleLine="true"
                    app:menu_fab_size="mini"
                    app:menu_openDirection="down"
                    app:menu_colorNormal="@color/ColorPrimary3"
                    app:menu_colorPressed="@color/ColorPrimary3"
                    app:menu_showShadow="false"
                    app:menu_labels_position="right">

                    <com.github.clans.fab.FloatingActionButton
                        android:id="@+id/hilowcontent"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/chevron_right"
                        app:fab_size="mini"
                        app:fab_colorNormal="@color/ColorAccent"
                        app:fab_colorPressed="@color/ColorAccentDark" />
                    <!--app:fab_label="High/Low"-->

                    <com.github.clans.fab.FloatingActionButton
                        android:id="@+id/lowhicontent"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/chevron_left"
                        app:fab_size="mini"
                        app:fab_colorNormal="@color/ColorAccent"
                        app:fab_colorPressed="@color/ColorAccentDark"/>
                    <!--app:fab_label="Low/High"-->

                    <com.github.clans.fab.FloatingActionButton
                        android:id="@+id/rangecontent"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/code_tags"
                        app:fab_size="mini"
                        app:fab_colorNormal="@color/ColorAccent"
                        app:fab_colorPressed="@color/ColorAccentDark"/>
                    <!--app:fab_label="Range"-->

                </com.github.clans.fab.FloatingActionMenu>
                <com.github.clans.fab.FloatingActionMenu
                    android:id="@+id/fabprice"
                    app:menu_icon="@drawable/currency_usd"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_alignParentRight="false"
                    android:layout_marginTop="50dp"
                    android:layout_marginLeft="0dp"
                    android:paddingRight="10dp"
                    android:paddingTop="10dp"
                    android:paddingBottom="10dp"
                    android:paddingLeft="10dp"
                    android:elevation="9dp"
                    app:menu_buttonSpacing="7dp"
                    app:menu_labels_ellipsize="end"
                    app:menu_labels_singleLine="true"
                    app:menu_fab_size="mini"
                    app:menu_openDirection="down"
                    app:menu_colorNormal="@color/ColorPrimary3"
                    app:menu_colorPressed="@color/ColorPrimary3"
                    app:menu_showShadow="false"
                    app:menu_labels_position="right">

                    <com.github.clans.fab.FloatingActionButton
                        android:id="@+id/hilow"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/chevron_right"
                        app:fab_size="mini"
                        app:fab_colorNormal="@color/ColorAccent"
                        app:fab_colorPressed="@color/ColorAccentDark" />
                    <!--app:fab_label="High/Low"-->

                    <com.github.clans.fab.FloatingActionButton
                        android:id="@+id/lowhi"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/chevron_left"
                        app:fab_size="mini"
                        app:fab_colorNormal="@color/ColorAccent"
                        app:fab_colorPressed="@color/ColorAccentDark"/>
                    <!--app:fab_label="Low/High"-->

                    <com.github.clans.fab.FloatingActionButton
                        android:id="@+id/pricerange"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/code_tags"
                        app:fab_size="mini"
                        app:fab_colorNormal="@color/ColorAccent"
                        app:fab_colorPressed="@color/ColorAccentDark"/>
                    <!--app:fab_label="Range"-->

                </com.github.clans.fab.FloatingActionMenu>
                <com.github.clans.fab.FloatingActionMenu
                    android:id="@+id/fabtype"
                    app:menu_icon="@drawable/beer"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_alignParentRight="false"
                    android:layout_marginTop="50dp"
                    android:layout_marginLeft="0dp"
                    android:paddingRight="10dp"
                    android:paddingTop="10dp"
                    android:paddingBottom="10dp"
                    android:paddingLeft="10dp"
                    android:elevation="9dp"
                    app:menu_buttonSpacing="7dp"
                    app:menu_labels_ellipsize="end"
                    app:menu_labels_singleLine="true"
                    app:menu_fab_size="mini"
                    app:menu_openDirection="down"
                    app:menu_colorNormal="@color/ColorPrimary3"
                    app:menu_colorPressed="@color/ColorPrimary3"
                    app:menu_showShadow="false"
                    app:menu_labels_position="right">

                    <com.github.clans.fab.FloatingActionButton
                        android:id="@+id/wines"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/glass_tulip"
                        app:fab_size="mini"
                        app:fab_colorNormal="@color/ColorAccent"
                        app:fab_colorPressed="@color/ColorAccentDark" />
                    <!--app:fab_label="Wines"-->

                    <com.github.clans.fab.FloatingActionButton
                        android:id="@+id/beers"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/glass_mug"
                        app:fab_size="mini"
                        app:fab_colorNormal="@color/ColorAccent"
                        app:fab_colorPressed="@color/ColorAccentDark"/>
                    <!--app:fab_label="Beers"-->

                    <com.github.clans.fab.FloatingActionButton
                        android:id="@+id/liquors"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/martini"
                        app:fab_size="mini"
                        app:fab_colorNormal="@color/ColorAccent"
                        app:fab_colorPressed="@color/ColorAccentDark"/>
                    <!--app:fab_label="Liquors"-->

                </com.github.clans.fab.FloatingActionMenu>

                <com.github.clans.fab.FloatingActionMenu
                    android:id="@+id/fabdist"
                    app:menu_icon="@drawable/car"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_alignParentRight="false"
                    android:layout_marginTop="50dp"
                    android:layout_marginLeft="0dp"
                    android:paddingRight="10dp"
                    android:paddingTop="10dp"
                    android:paddingBottom="10dp"
                    android:paddingLeft="10dp"
                    android:elevation="9dp"
                    app:menu_buttonSpacing="7dp"
                    app:menu_labels_ellipsize="end"
                    app:menu_labels_singleLine="true"
                    app:menu_fab_size="mini"
                    app:menu_openDirection="down"
                    app:menu_colorNormal="@color/ColorPrimary3"
                    app:menu_colorPressed="@color/ColorPrimary3"
                    app:menu_showShadow="false"
                    app:menu_labels_position="right">

                    <com.github.clans.fab.FloatingActionButton
                        android:id="@+id/custommi"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/star"
                        app:fab_label="Custom"
                        app:fab_size="mini"
                        app:fab_colorNormal="@color/ColorAccent"
                        app:fab_colorPressed="@color/ColorAccentDark" />

                    <com.github.clans.fab.FloatingActionButton
                        android:id="@+id/tenmi"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/star"
                        app:fab_label="10mi"
                        app:fab_size="mini"
                        app:fab_colorNormal="@color/ColorAccent"
                        app:fab_colorPressed="@color/ColorAccentDark"/>

                    <com.github.clans.fab.FloatingActionButton
                        android:id="@+id/fivemi"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/star"
                        app:fab_label="5mi"
                        app:fab_size="mini"
                        app:fab_colorNormal="@color/ColorAccent"
                        app:fab_colorPressed="@color/ColorAccentDark"/>

                    <com.github.clans.fab.FloatingActionButton
                        android:id="@+id/twomi"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/star"
                        app:fab_label="2mi"
                        app:fab_size="mini"
                        app:fab_colorNormal="@color/ColorAccent"
                        app:fab_colorPressed="@color/ColorAccentDark"/>

                </com.github.clans.fab.FloatingActionMenu>

                <com.github.clans.fab.FloatingActionButton
                    android:id="@+id/fabtop"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="bottom|right"
                    app:fab_showShadow="true"
                    android:layout_marginRight="10dp"
                    android:layout_marginBottom="10dp"
                    android:src="@drawable/camera"
                    app:fab_showAnimation="@anim/fab_scale_up"
                    app:fab_hideAnimation="@anim/fab_scale_down"
                    app:fab_colorNormal="@color/ColorPrimary"
                    app:fab_colorPressed="@color/ColorPrimaryDark" />

            </FrameLayout>

            <com.quinny898.library.persistentsearch.SearchBox
                android:layout_width="wrap_content"
                android:id="@+id/searchbox"
                android:layout_height="wrap_content"
                android:visibility="gone"
                android:layout_marginTop="-4dp"
                android:layout_marginLeft="-6dp"
                android:layout_marginRight="-6dp"
                android:layout_alignParentLeft="true"
                android:layout_alignParentTop="true" />
        </FrameLayout>


    </RelativeLayout>

    <android.support.design.widget.NavigationView
        android:id="@+id/navigation_view"
        android:layout_height="match_parent"
        android:layout_width="wrap_content"
        android:layout_gravity="start"
        app:headerLayout="@layout/header"
        app:menu="@menu/drawer"
        />
</android.support.v4.widget.DrawerLayout>

